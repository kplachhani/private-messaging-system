{
  "name": "node_typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint:fix": "tslint --fix --project tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "start:dev": "nodemon  --exec \"npm run lint && ts-node -r tsconfig-paths/register\" ./src/Index.ts",
    "build:ts": "npm run clean:win && tsc",
    "build:webpack": "npm run clean:win && webpack",
    "debug": "nodemon --exec ts-node  -r tsconfig-paths/register ./src/Index.ts",
    "debug:vs": "nodemon --watch src/**/* -e ts,json --exec node --inspect-brk -r ts-node/register -r tsconfig-paths/register ./src/Index.ts",
    "debug:ts": "ts-node  -r tsconfig-paths/register ./src/Index.ts",
    "clean": "rm -rf dist && mkdir dist",
    "clean:win": "rimraf dist && mkdir dist",
    "start": "npm run lint && npm run build:webpack && npm run exec",
    "start:ts": "npm run lint && npm run build:ts && npm run exec:ts",
    "exec": "node ./dist/api.bundle.js",
    "exec:ts": "node --inspect -r ./tsconfig-paths-bootstrap.js ./dist/Index.js",
    "debug:thread": "nodemon -e ts  --exec \" npm run start:ts \""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "socket.io": "^2.3.0",
    "socket.io-emitter": "^3.1.1",
    "socket.io-redis": "^5.2.0",
    "async": "^3.2.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.6",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-emitter": "^3.1.0",
    "@types/socket.io-redis": "^1.0.25",
    "@types/winston": "^2.4.4",
    "nodemon": "^2.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
